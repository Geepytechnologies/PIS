{
  "expo": {
    "name": "Enpis",
    "slug": "Enpis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/myicon.png",
    "scheme": "enpis",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.geepytech.Enpis"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsVersion": "11.0.0",
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoiZ2VlcHl0ZWNoIiwiYSI6ImNtZnBreW5sdTBneDYyanM4YzY5ZjRxZ3UifQ.YKgBnWMLEhznDgcPDdfmIg"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mysplash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000301",
          "dark": {
            "backgroundColor": "#000301"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
